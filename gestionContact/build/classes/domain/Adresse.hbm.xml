<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="domain">
  <class name="Adresse" table="ADRESSE">
  	 <id name="idAdresse" type="long" column="ID_ADRESSE" >
   		<generator class="increment"/>
  	 </id>
  	  <property name="rue" type="string" not-null="true" lazy="false" /> 
  	  <property name="codepostal" type="string" not-null="true" lazy="false" /> 
  	  <property name="ville" type="string" not-null="true" lazy="false" /> 
  	  <property name="pays" type="string" not-null="true" lazy="false" /> 

  </class>
</hibernate-mapping>
